{
  "metadata": [
    {
      "src": "FlaxEngine/Binaries/Editor/Win64/Development/FlaxEngine.CSharp.dll",
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "api-cpp/**.yml"
        ]
      },
      {
        "files": [
          "manual/**/*.md",
          "manual/**/toc.yml",
          "api/*.md",
          "api/*.yml",
          "api-cpp/*.md",
          "api-cpp/*.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**/*.png", 
          "**/media/**/*.jpg", 
          "**/media/**/*.gif", 
          "**/media/**/*.mp4", 
          "**/media/**/*.zip", 
          "**/media/**/*.svg",
          "**/media/**/*.cs"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Flax Documentation",
      "_appFooter": "Copyright Â© 2012-2023 Wojciech Figat",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_gitUrlPattern": "github",
      //"_gitContribute": {
      //		"repo ": "https://github.com/FlaxEngine/FlaxDocs",
      //		"branch": "master"
      //}
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}